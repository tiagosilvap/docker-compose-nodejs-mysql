<div class="posts">
    <h1>Full Cycle Rocks!</h1>
    <div class="row justify-content-center">
        {{#each data}}
        <div class="col-lg-7" style="margin-top: 50px;">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">{{this.title}}</h5>
                    <ul class="list-group">
                        <li class="list-group-item">{{this.description}}</li>
                    </ul>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>